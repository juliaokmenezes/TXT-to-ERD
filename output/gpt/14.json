{
  "tables": {
    "AgenteImobiliario": {
      "*id_agente": "int() NOT NULL",
      "nome": "varchar(200) NOT NULL",
      "email": "varchar(200)",
      "telefone": "varchar(15)"
    },
    "Proprietario": {
      "*id_proprietario": "int() NOT NULL",
      "nome": "varchar(200) NOT NULL",
      "email": "varchar(200)",
      "telefone": "varchar(15)"
    },
    "Inquilino": {
      "*id_inquilino": "int() NOT NULL",
      "nome": "varchar(200) NOT NULL",
      "email": "varchar(200)",
      "telefone": "varchar(15)",
      "renda_comprovada": "decimal(10,2)"
    },
    "Propriedade": {
      "*id_propriedade": "int() NOT NULL",
      "+id_agente": "int()",
      "+id_proprietario": "int()",
      "endereco": "varchar(500) NOT NULL",
      "valor_aluguel": "decimal(10,2) NOT NULL",
      "ultima_manutencao": "date()"
    },
    "ContratoAluguel": {
      "*id_contrato": "int() NOT NULL",
      "+id_inquilino": "int()",
      "+id_propriedade": "int()",
      "+id_fiador": "int()",
      "data_inicio": "date() NOT NULL",
      "data_fim": "date()",
      "valor_mensal": "decimal(10,2) NOT NULL"
    },
    "Pagamento": {
      "*id_pagamento": "int() NOT NULL",
      "+id_contrato": "int()",
      "data_pagamento": "date() NOT NULL",
      "valor": "decimal(10,2) NOT NULL",
      "pagador": "varchar(200) NOT NULL",
      "recebedor": "varchar(200) NOT NULL"
    },
    "Fiador": {
      "*id_fiador": "int() NOT NULL",
      "nome": "varchar(200) NOT NULL",
      "email": "varchar(200)",
      "telefone": "varchar(15)"
    },
    "Indicacao": {
      "*id_indicacao": "int() NOT NULL",
      "+id_agente_indicador": "int()",
      "+id_agente_indicado": "int()",
      "data_indicacao": "date() NOT NULL",
      "comissao": "decimal(10,2)"
    },
    "DanoMulta": {
      "*id_dano_multa": "int() NOT NULL",
      "+id_contrato": "int()",
      "motivo": "text() NOT NULL",
      "evidencias": "text()",
      "valor_multa": "decimal(10,2)"
    }
  },
  "relations": [
    "Propriedade:id_agente 1--* AgenteImobiliario:id_agente",
    "Propriedade:id_proprietario 1--* Proprietario:id_proprietario",
    "ContratoAluguel:id_inquilino 1--* Inquilino:id_inquilino",
    "ContratoAluguel:id_propriedade 1--* Propriedade:id_propriedade",
    "ContratoAluguel:id_fiador ?--* Fiador:id_fiador",
    "Pagamento:id_contrato 1--* ContratoAluguel:id_contrato",
    "Indicacao:id_agente_indicador 1--* AgenteImobiliario:id_agente",
    "Indicacao:id_agente_indicado 1--* AgenteImobiliario:id_agente",
    "DanoMulta:id_contrato 1--* ContratoAluguel:id_contrato"
  ],
  "rankAdjustments": "",
  "label": "Sistema de Gest\u00e3o Imobili\u00e1ria"
}