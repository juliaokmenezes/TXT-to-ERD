{
  "tables": {
    "AgenteImobiliario": {
      "*id_agente": "int() NOT NULL",
      "nome": "varchar(200) NOT NULL",
      "telefone": "varchar(15)"
    },
    "Proprietario": {
      "*id_proprietario": "int() NOT NULL",
      "nome": "varchar(200) NOT NULL",
      "telefone": "varchar(15)"
    },
    "Inquilino": {
      "*id_inquilino": "int() NOT NULL",
      "nome": "varchar(200) NOT NULL",
      "telefone": "varchar(15)",
      "renda_comprovada": "decimal(10,2)"
    },
    "Propriedade": {
      "*id_propriedade": "int() NOT NULL",
      "+id_agente": "int()",
      "+id_proprietario": "int()",
      "endereco": "varchar(500) NOT NULL",
      "estado": "varchar(100)",
      "alugavel": "boolean() NOT NULL DEFAULT true"
    },
    "ContratoAluguel": {
      "*id_contrato": "int() NOT NULL",
      "+id_inquilino": "int()",
      "+id_propriedade": "int()",
      "+id_fiador": "int()",
      "data_inicio": "date() NOT NULL",
      "data_fim": "date()",
      "valor_aluguel": "decimal(10,2) NOT NULL"
    },
    "Pagamento": {
      "*id_pagamento": "int() NOT NULL",
      "+id_contrato": "int()",
      "data_pagamento": "date() NOT NULL",
      "valor": "decimal(10,2) NOT NULL",
      "pagador": "varchar(200) NOT NULL",
      "recebedor": "varchar(200) NOT NULL"
    },
    "Fiador": {
      "*id_fiador": "int() NOT NULL",
      "nome": "varchar(200) NOT NULL",
      "telefone": "varchar(15)"
    },
    "Inspecao": {
      "*id_inspecao": "int() NOT NULL",
      "+id_propriedade": "int()",
      "data_inspecao": "date() NOT NULL",
      "danos": "text()",
      "reparos_realizados": "boolean() NOT NULL DEFAULT false"
    }
  },
  "relations": [
    "AgenteImobiliario:id_agente 1--* Propriedade:id_agente",
    "Proprietario:id_proprietario 1--* Propriedade:id_proprietario",
    "Inquilino:id_inquilino 1--* ContratoAluguel:id_inquilino",
    "Propriedade:id_propriedade 1--* ContratoAluguel:id_propriedade",
    "Fiador:id_fiador 0--1 ContratoAluguel:id_fiador",
    "ContratoAluguel:id_contrato 1--* Pagamento:id_contrato",
    "Propriedade:id_propriedade 1--* Inspecao:id_propriedade"
  ],
  "rankAdjustments": "",
  "label": "Sistema de Gest\u00e3o Imobili\u00e1ria"
}