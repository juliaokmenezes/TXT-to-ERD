{
  "tables": {
    "Agent": {
      "*agent_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "contact_info": "varchar(200)",
      "commission_rate": "decimal(5,2) NOT NULL"
    },
    "Renter": {
      "*renter_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "contact_info": "varchar(200)",
      "cpf": "varchar(11) NOT NULL UNIQUE"
    },
    "Property": {
      "*property_id": "int() NOT NULL",
      "address": "varchar(200) NOT NULL",
      "price": "decimal(10,2) NOT NULL",
      "status": "varchar(50) NOT NULL",
      "type": "varchar(50) NOT NULL",
      "+agent_id": "int()",
      "+owner_id": "int()"
    },
    "PropertyOwner": {
      "*owner_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "contact_info": "varchar(200)",
      "cpf": "varchar(11) NOT NULL UNIQUE"
    },
    "RentPayment": {
      "*payment_id": "int() NOT NULL",
      "amount": "decimal(10,2) NOT NULL",
      "payment_date": "date() NOT NULL",
      "month": "varchar(20) NOT NULL",
      "status": "varchar(50) NOT NULL",
      "+renter_id": "int()",
      "+property_id": "int()",
      "commission_percent": "decimal(5,2) NOT NULL"
    }
  },
  "relations": [
    "Agent:agent_id 1--* Property:agent_id",
    "PropertyOwner:owner_id 1--* Property:owner_id",
    "Renter:renter_id *--* Property:property_id",
    "RentPayment:payment_id *--1 Renter:renter_id",
    "RentPayment:payment_id *--1 Property:property_id"
  ],
  "rankAdjustments": "",
  "label": "Real Estate Agency Database"
}