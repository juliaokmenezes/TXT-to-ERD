{
  "tables": {
    "Customer": {
      "*customer_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100) NOT NULL",
      "phone": "varchar(20) NOT NULL",
      "+address_id": "int()"
    },
    "Address": {
      "*address_id": "int() NOT NULL",
      "street": "varchar(100) NOT NULL",
      "city": "varchar(100) NOT NULL",
      "state": "varchar(100) NOT NULL",
      "country": "varchar(100) NOT NULL"
    },
    "Order": {
      "*order_id": "int() NOT NULL",
      "+customer_id": "int()",
      "order_date": "date() NOT NULL",
      "total": "decimal(10,2) NOT NULL"
    },
    "OrderItem": {
      "*order_item_id": "int() NOT NULL",
      "+order_id": "int()",
      "+product_id": "int()",
      "quantity": "int() NOT NULL",
      "price": "decimal(10,2) NOT NULL"
    },
    "Product": {
      "*product_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "description": "text() NOT NULL",
      "price": "decimal(10,2) NOT NULL"
    }
  },
  "relations": [
    "Customer:customer_id *--1 Address:address_id",
    "Order:customer_id *--1 Customer:customer_id",
    "Order:order_id *--1 OrderItem:order_item_id",
    "OrderItem:order_id *--1 Order:order_id",
    "OrderItem:product_id *--1 Product:product_id"
  ],
  "rankAdjustments": "",
  "label": "E-commerce Database"
}