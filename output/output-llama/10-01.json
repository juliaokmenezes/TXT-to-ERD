{
  "tables": {
    "Agent": {
      "*agent_id": "int() NOT NULL",
      "first_name": "varchar(100)",
      "last_name": "varchar(100)",
      "email": "varchar(100) NOT NULL",
      "phone_number": "varchar(20)"
    },
    "Renter": {
      "*renter_id": "int() NOT NULL",
      "first_name": "varchar(100)",
      "last_name": "varchar(100)",
      "email": "varchar(100) NOT NULL",
      "phone_number": "varchar(20)",
      "salary": "decimal(10,2) NOT NULL"
    },
    "Property": {
      "*property_id": "int() NOT NULL",
      "address": "varchar(200) NOT NULL",
      "city": "varchar(100) NOT NULL",
      "state": "varchar(100) NOT NULL",
      "zip": "varchar(10) NOT NULL"
    },
    "PropertyOwner": {
      "*property_owner_id": "int() NOT NULL",
      "first_name": "varchar(100)",
      "last_name": "varchar(100)",
      "email": "varchar(100) NOT NULL",
      "phone_number": "varchar(20)"
    },
    "Contract": {
      "*contract_id": "int() NOT NULL",
      "+agent_id": "int()",
      "+renter_id": "int()",
      "+property_id": "int()",
      "start_date": "date() NOT NULL",
      "end_date": "date() NOT NULL",
      "clauses": "text() NOT NULL",
      "security_deposit_refunded": "boolean() NOT NULL"
    },
    "RentPayment": {
      "*rent_payment_id": "int() NOT NULL",
      "+contract_id": "int()",
      "payment_date": "date() NOT NULL",
      "payment_account": "varchar(100) NOT NULL",
      "receiving_account": "varchar(100) NOT NULL"
    },
    "Guarantor": {
      "*guarantor_id": "int() NOT NULL",
      "+renter_id": "int()",
      "+contract_id": "int()"
    },
    "Maintenance": {
      "*maintenance_id": "int() NOT NULL",
      "+property_id": "int()",
      "maintenance_date": "date() NOT NULL"
    },
    "Fine": {
      "*fine_id": "int() NOT NULL",
      "+contract_id": "int()",
      "reason": "varchar(200) NOT NULL",
      "evidence": "text() NOT NULL"
    },
    "Broker": {
      "*broker_id": "int() NOT NULL",
      "first_name": "varchar(100)",
      "last_name": "varchar(100)",
      "email": "varchar(100) NOT NULL",
      "phone_number": "varchar(20)"
    },
    "Referral": {
      "*referral_id": "int() NOT NULL",
      "+broker_id": "int()",
      "+referring_broker_id": "int()",
      "referral_date": "date() NOT NULL",
      "commission": "decimal(10,2) NOT NULL"
    }
  },
  "relations": [
    "Agent:agent_id *--1 Contract:agent_id",
    "Renter:renter_id *--1 Contract:renter_id",
    "Property:property_id *--1 Contract:property_id",
    "PropertyOwner:property_owner_id *--1 Property:property_id",
    "Contract:agent_id *--1 Agent:agent_id",
    "Contract:renter_id *--1 Renter:renter_id",
    "Contract:property_id *--1 Property:property_id",
    "Guarantor:renter_id *--1 Renter:renter_id",
    "Guarantor:contract_id *--1 Contract:contract_id",
    "Maintenance:property_id *--1 Property:property_id",
    "Fine:contract_id *--1 Contract:contract_id",
    "Broker:broker_id *--1 Referral:broker_id",
    "Referral:referring_broker_id *--1 Broker:broker_id"
  ],
  "rankAdjustments": "",
  "label": "Real Estate Agency Database"
}