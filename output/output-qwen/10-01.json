{
  "tables": {
    "Agent": {
      "*agent_id": "int() NOT NULL",
      "first_name": "varchar(100)",
      "last_name": "varchar(100)",
      "email": "varchar(100)",
      "phone": "varchar(20)"
    },
    "Property": {
      "*property_id": "int() NOT NULL",
      "address": "varchar(200)",
      "city": "varchar(100)",
      "state": "varchar(100)",
      "zip_code": "varchar(20)",
      "last_maintenance_date": "date()"
    },
    "Owner": {
      "*owner_id": "int() NOT NULL",
      "first_name": "varchar(100)",
      "last_name": "varchar(100)",
      "email": "varchar(100)",
      "phone": "varchar(20)"
    },
    "Renter": {
      "*renter_id": "int() NOT NULL",
      "first_name": "varchar(100)",
      "last_name": "varchar(100)",
      "email": "varchar(100)",
      "phone": "varchar(20)",
      "salary": "decimal(10,2)"
    },
    "Contract": {
      "*contract_id": "int() NOT NULL",
      "+renter_id": "int()",
      "+property_id": "int()",
      "start_date": "date()",
      "end_date": "date()",
      "clauses": "text()",
      "deposit_refunded": "boolean()",
      "guarantor_required": "boolean()",
      "maintenance_flag": "boolean()"
    },
    "Payment": {
      "*payment_id": "int() NOT NULL",
      "+contract_id": "int()",
      "payment_date": "date()",
      "amount": "decimal(10,2)",
      "month": "varchar(20)",
      "payer_account": "varchar(50)",
      "receiver_account": "varchar(50)"
    },
    "Referral": {
      "*referral_id": "int() NOT NULL",
      "+agent_id": "int()",
      "+referred_agent_id": "int()",
      "referral_date": "date()",
      "commission": "decimal(10,2)"
    },
    "Fine": {
      "*fine_id": "int() NOT NULL",
      "+property_id": "int()",
      "reason": "text()",
      "evidence": "text()",
      "first_offense": "boolean()"
    },
    "PropertyOwnership": {
      "*ownership_id": "int() NOT NULL",
      "+owner_id": "int()",
      "+property_id": "int()"
    },
    "PropertyManagement": {
      "*management_id": "int() NOT NULL",
      "+agent_id": "int()",
      "+property_id": "int()"
    }
  },
  "relations": [
    "Contract:renter_id 1--* Renter:renter_id",
    "Contract:property_id 1--* Property:property_id",
    "Payment:contract_id 1--* Contract:contract_id",
    "Referral:agent_id 1--* Agent:agent_id",
    "Referral:referred_agent_id 1--* Agent:agent_id",
    "Fine:property_id 1--* Property:property_id",
    "PropertyOwnership:owner_id 1--* Owner:owner_id",
    "PropertyOwnership:property_id 1--* Property:property_id",
    "PropertyManagement:agent_id 1--* Agent:agent_id",
    "PropertyManagement:property_id 1--* Property:property_id"
  ],
  "rankAdjustments": "",
  "label": "Real Estate Agency Database"
}