{
  "tables": {
    "Agent": {
      "*agent_id": "int() NOT NULL",
      "agent_name": "varchar(100) NOT NULL",
      "agent_email": "varchar(100)",
      "agent_phone": "varchar(20)"
    },
    "Property": {
      "*property_id": "int() NOT NULL",
      "+agent_id": "int()",
      "property_address": "varchar(255) NOT NULL",
      "property_type": "varchar(50)",
      "property_price": "decimal(10,2)"
    },
    "Owner": {
      "*owner_id": "int() NOT NULL",
      "owner_name": "varchar(100) NOT NULL",
      "owner_email": "varchar(100)",
      "owner_phone": "varchar(20)"
    },
    "Renter": {
      "*renter_id": "int() NOT NULL",
      "renter_name": "varchar(100) NOT NULL",
      "renter_email": "varchar(100)",
      "renter_phone": "varchar(20)"
    },
    "Rental": {
      "*rental_id": "int() NOT NULL",
      "+renter_id": "int()",
      "+property_id": "int()",
      "rental_start_date": "date() NOT NULL",
      "rental_end_date": "date()"
    },
    "Payment": {
      "*payment_id": "int() NOT NULL",
      "+rental_id": "int()",
      "payment_amount": "decimal(10,2) NOT NULL",
      "payment_date": "date() NOT NULL",
      "payment_month": "varchar(20) NOT NULL"
    }
  },
  "relations": [
    "Property:agent_id *--1 Agent:agent_id",
    "Property:owner_id *--1 Owner:owner_id",
    "Rental:renter_id *--1 Renter:renter_id",
    "Rental:property_id *--1 Property:property_id",
    "Payment:rental_id *--1 Rental:rental_id"
  ],
  "rankAdjustments": "",
  "label": "Real Estate Agency Database"
}