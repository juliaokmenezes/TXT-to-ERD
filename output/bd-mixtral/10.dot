    </table>>];

    
    "Agent":"id_agent"->"Property":"ownerID" [
        arrowhead=ocrow,

        arrowtail=noneotee,
    ];

    
    "Owner":"id_owner"->"Property":"id_property" [
        arrowhead=ocrow,

        arrowtail=ocrow,
    ];

    
    "Tenant":"id_tenant"->"Contract":"tenantID" [
