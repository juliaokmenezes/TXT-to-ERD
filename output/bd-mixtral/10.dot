    ];

    
    "Contract":"propertyID"->"Property":"id_property" [
        arrowhead=noneotee,

        arrowtail=noneotee,
    ];

    
    "Contract":"tenantID"->"Tenant":"id_tenant" [
        arrowhead=noneotee,

        arrowtail=noneotee,
