{
  "tables": {
    "Agent": {
      "*agent_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100) NOT NULL",
      "phone": "varchar(20)"
    },
    "Property": {
      "*property_id": "int() NOT NULL",
      "+agent_id": "int()",
      "+owner_id": "int()",
      "address": "varchar(200) NOT NULL",
      "type": "varchar(50) NOT NULL"
    },
    "Owner": {
      "*owner_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100) NOT NULL",
      "phone": "varchar(20)"
    },
    "Renter": {
      "*renter_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100) NOT NULL",
      "phone": "varchar(20)"
    },
    "RentalContract": {
      "*contract_id": "int() NOT NULL",
      "+property_id": "int()",
      "+renter_id": "int()",
      "start_date": "date() NOT NULL",
      "end_date": "date()",
      "rent": "decimal(10,2) NOT NULL"
    },
    "Payment": {
      "*payment_id": "int() NOT NULL",
      "+contract_id": "int()",
      "payment_method": "varchar(50) NOT NULL",
      "payment_date": "date() NOT NULL",
      "amount": "decimal(10,2) NOT NULL"
    },
    "Maintenance": {
      "*maintenance_id": "int() NOT NULL",
      "+property_id": "int()",
      "maintenance_date": "date() NOT NULL"
    },
    "Indication": {
      "*indication_id": "int() NOT NULL",
      "+agent_id": "int()",
      "+indicated_agent_id": "int()",
      "indication_date": "date() NOT NULL",
      "commission": "decimal(10,2)"
    }
  },
  "relations": [
    "Agent:agent_id 1--* Property:agent_id",
    "Owner:owner_id 1--* Property:owner_id",
    "Renter:renter_id 1--* RentalContract:renter_id",
    "Property:property_id 1--* RentalContract:property_id",
    "RentalContract:contract_id 1--* Payment:contract_id",
    "Property:property_id 1--* Maintenance:property_id",
    "Agent:agent_id 1--* Indication:agent_id",
    "Agent:agent_id 1--* Indication:indicated_agent_id"
  ],
  "rankAdjustments": "",
  "label": "Real Estate Management System"
}