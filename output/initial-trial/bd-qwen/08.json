{
  "tables": {
    "Agent": {
      "*agentID": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)"
    },
    "Property": {
      "*propertyID": "int() NOT NULL",
      "address": "varchar(255) NOT NULL",
      "type": "varchar(50)",
      "lastMaintenanceDate": "date()",
      "+agentID": "int()",
      "+ownerID": "int()"
    },
    "Owner": {
      "*ownerID": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)"
    },
    "Tenant": {
      "*tenantID": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)",
      "salary": "decimal(10,2)"
    },
    "Contract": {
      "*contractID": "int() NOT NULL",
      "startDate": "date() NOT NULL",
      "endDate": "date() NOT NULL",
      "clauses": "text",
      "depositRefunded": "boolean",
      "+propertyID": "int()",
      "+tenantID": "int()"
    },
    "RentPayment": {
      "*paymentID": "int() NOT NULL",
      "paymentDate": "date() NOT NULL",
      "amount": "decimal(10,2) NOT NULL",
      "month": "int() NOT NULL",
      "paymentAccount": "varchar(50)",
      "receivingAccount": "varchar(50)",
      "+contractID": "int()"
    },
    "Guarantor": {
      "*guarantorID": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)",
      "percentageCovered": "decimal(5,2) NOT NULL",
      "+contractID": "int()"
    }
  },
  "relations": [
    "Property:agentID *--1 Agent:agentID",
    "Property:ownerID *--1 Owner:ownerID",
    "Contract:propertyID *--1 Property:propertyID",
    "Contract:tenantID *--1 Tenant:tenantID",
    "RentPayment:contractID *--1 Contract:contractID",
    "Guarantor:contractID *--1 Contract:contractID"
  ],
  "rankAdjustments": [],
  "label": "Real Estate Agency Database Schema"
}