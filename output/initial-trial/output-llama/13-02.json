{
  "tables": {
    "Agent": {
      "*username": "varchar(100) NOT NULL",
      "first_name": "varchar(100)",
      "last_name": "varchar(100)",
      "password": "varchar(100) NOT NULL",
      "role": "varchar(20)"
    },
    "Property": {
      "*id_property": "int() NOT NULL",
      "address": "varchar(200) NOT NULL",
      "owner_id": "int()"
    },
    "Renter": {
      "*id_renter": "int() NOT NULL",
      "first_name": "varchar(100)",
      "last_name": "varchar(100)",
      "salary": "decimal(10,2)"
    },
    "Contract": {
      "*id_contract": "int() NOT NULL",
      "+agent_username": "varchar(100)",
      "+renter_id": "int()",
      "start_date": "date() NOT NULL",
      "end_date": "date() NOT NULL",
      "clauses": "text() NOT NULL",
      "security_deposit_refunded": "boolean() NOT NULL"
    },
    "Payment": {
      "*id_payment": "int() NOT NULL",
      "+contract_id": "int()",
      "payment_date": "date() NOT NULL",
      "payment_method": "varchar(20) NOT NULL"
    },
    "Guarantor": {
      "*id_guarantor": "int() NOT NULL",
      "+contract_id": "int()",
      "percentage": "decimal(10,2) NOT NULL"
    }
  },
  "relations": [
    "Agent:username *--1 Contract:agent_username",
    "Contract:renter_id *--1 Renter:id_renter",
    "Property:owner_id *--1 Agent:username",
    "Contract:agent_username *--1 Agent:username",
    "Payment:contract_id *--1 Contract:id_contract",
    "Guarantor:contract_id *--1 Contract:id_contract"
  ],
  "rankAdjustments": "",
  "label": "Real Estate Agency"
}