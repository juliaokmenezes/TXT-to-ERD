{
  "tables": {
    "Agent": {
      "*agent_id": "int() NOT NULL",
      "first_name": "varchar(100)",
      "last_name": "varchar(100)",
      "email": "varchar(100)"
    },
    "Property": {
      "*property_id": "int() NOT NULL",
      "address": "varchar(200)",
      "city": "varchar(100)",
      "state": "varchar(100)",
      "country": "varchar(100)"
    },
    "PropertyOwner": {
      "*property_owner_id": "int() NOT NULL",
      "first_name": "varchar(100)",
      "last_name": "varchar(100)",
      "email": "varchar(100)"
    },
    "Renter": {
      "*renter_id": "int() NOT NULL",
      "first_name": "varchar(100)",
      "last_name": "varchar(100)",
      "email": "varchar(100)"
    },
    "RentalAgreement": {
      "*agreement_id": "int() NOT NULL",
      "+agent_id": "int()",
      "+property_id": "int()",
      "+renter_id": "int()",
      "start_date": "date() NOT NULL",
      "end_date": "date() NOT NULL",
      "security_deposit_refunded": "boolean() NOT NULL",
      "penalty_amount": "decimal(10,2) NOT NULL"
    },
    "RentPayment": {
      "*payment_id": "int() NOT NULL",
      "+agreement_id": "int()",
      "payment_date": "date() NOT NULL",
      "payment_amount": "decimal(10,2) NOT NULL"
    }
  },
  "relations": [
    "Agent:agent_id *--1 RentalAgreement:agent_id",
    "Property:property_id *--1 RentalAgreement:property_id",
    "PropertyOwner:property_owner_id *--1 Property:property_id",
    "Renter:renter_id *--1 RentalAgreement:renter_id",
    "RentalAgreement:agreement_id *--1 RentPayment:agreement_id"
  ],
  "rankAdjustments": "",
  "label": "Real Estate Agency Database"
}