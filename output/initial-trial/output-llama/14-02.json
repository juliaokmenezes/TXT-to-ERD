{
  "tables": {
    "Agent": {
      "*agent_id": "int() NOT NULL",
      "first_name": "varchar(100)",
      "last_name": "varchar(100)",
      "email": "varchar(100) NOT NULL",
      "phone": "varchar(20)"
    },
    "Renter": {
      "*renter_id": "int() NOT NULL",
      "first_name": "varchar(100)",
      "last_name": "varchar(100)",
      "email": "varchar(100) NOT NULL",
      "phone": "varchar(20)",
      "salary": "decimal(10,2) NOT NULL"
    },
    "Property": {
      "*property_id": "int() NOT NULL",
      "address": "varchar(200) NOT NULL",
      "city": "varchar(100) NOT NULL",
      "state": "varchar(100) NOT NULL",
      "country": "varchar(100) NOT NULL"
    },
    "PropertyOwner": {
      "*property_owner_id": "int() NOT NULL",
      "first_name": "varchar(100)",
      "last_name": "varchar(100)",
      "email": "varchar(100) NOT NULL",
      "phone": "varchar(20)"
    },
    "Contract": {
      "*contract_id": "int() NOT NULL",
      "+agent_id": "int()",
      "+renter_id": "int()",
      "+property_id": "int()",
      "start_date": "date() NOT NULL",
      "end_date": "date() NOT NULL",
      "clauses": "text() NOT NULL",
      "security_deposit_refunded": "boolean() NOT NULL"
    },
    "Guarantor": {
      "*guarantor_id": "int() NOT NULL",
      "+contract_id": "int()",
      "percentage": "decimal(10,2) NOT NULL"
    },
    "Payment": {
      "*payment_id": "int() NOT NULL",
      "+contract_id": "int()",
      "payment_date": "date() NOT NULL",
      "payment_method": "varchar(20) NOT NULL"
    },
    "RentPriceHistory": {
      "*rent_price_id": "int() NOT NULL",
      "+property_id": "int()",
      "rent_price": "decimal(10,2) NOT NULL",
      "date": "date() NOT NULL"
    },
    "Review": {
      "*review_id": "int() NOT NULL",
      "+renter_id": "int()",
      "score": "integer() NOT NULL",
      "comment": "text() NOT NULL"
    }
  },
  "relations": [
    "Contract:agent_id *--1 Agent:agent_id",
    "Contract:renter_id *--1 Renter:renter_id",
    "Contract:property_id *--1 Property:property_id",
    "Guarantor:contract_id *--1 Contract:contract_id",
    "Payment:contract_id *--1 Contract:contract_id",
    "RentPriceHistory:property_id *--1 Property:property_id",
    "Review:renter_id *--1 Renter:renter_id"
  ],
  "rankAdjustments": "",
  "label": "Real Estate Agency"
}