{
  "tables": {
    "Agent": {
      "*agent_id": "int() NOT NULL",
      "first_name": "varchar(100)",
      "last_name": "varchar(100)",
      "email": "varchar(100)"
    },
    "Renter": {
      "*renter_id": "int() NOT NULL",
      "first_name": "varchar(100)",
      "last_name": "varchar(100)",
      "email": "varchar(100)",
      "salary": "decimal(10,2)"
    },
    "Property": {
      "*property_id": "int() NOT NULL",
      "address": "varchar(200)",
      "city": "varchar(100)",
      "state": "varchar(100)",
      "country": "varchar(100)"
    },
    "PropertyOwner": {
      "*property_owner_id": "int() NOT NULL",
      "first_name": "varchar(100)",
      "last_name": "varchar(100)",
      "email": "varchar(100)"
    },
    "Contract": {
      "*contract_id": "int() NOT NULL",
      "+agent_id": "int()",
      "+renter_id": "int()",
      "+property_id": "int()",
      "start_date": "date() NOT NULL",
      "end_date": "date() NOT NULL",
      "clauses": "text() NOT NULL",
      "security_deposit_refunded": "boolean() NOT NULL"
    },
    "RentPayment": {
      "*rent_payment_id": "int() NOT NULL",
      "+contract_id": "int()",
      "payment_date": "date() NOT NULL",
      "payment_account": "varchar(100)",
      "receiving_account": "varchar(100)"
    },
    "MaintenanceRecord": {
      "*maintenance_id": "int() NOT NULL",
      "+property_id": "int()",
      "date": "date() NOT NULL",
      "description": "text() NOT NULL",
      "cost": "decimal(10,2) NOT NULL",
      "responsible_contractor": "varchar(100)"
    }
  },
  "relations": [
    "Contract:agent_id *--1 Agent:agent_id",
    "Contract:renter_id *--1 Renter:renter_id",
    "Contract:property_id *--1 Property:property_id",
    "RentPayment:contract_id *--1 Contract:contract_id",
    "MaintenanceRecord:property_id *--1 Property:property_id"
  ],
  "rankAdjustments": "",
  "label": "Real Estate Agency Database"
}