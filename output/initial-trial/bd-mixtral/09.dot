        <tr><td port="propertyID" align="left" cellpadding="5">propertyID <font color="grey60">int() NOT NULL</font></td></tr>
    </table>>];

    
    "Agent":"idAgent"->"Property":"agentID" [
        arrowhead=ocrow,

        arrowtail=noneotee,
