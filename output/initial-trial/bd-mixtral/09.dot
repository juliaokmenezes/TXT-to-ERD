    ];

    
    "Contract":"idContract"->"Referral":"referredID" [
        arrowhead=noneotee,

        arrowtail=noneotee,
    ];

    
    "Referral":"idReferral"->"Agent":"idAgent" [
        arrowhead=noneotee,

        arrowtail=noneotee,
    ];

    
    "Property":"idProperty"->"Maintenance":"propertyID" [
        arrowhead=noneotee,

        arrowtail=ocrow,
    ];
