{
  "tables": {
    "Agent": {
      "*agent_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "phone": "varchar(20) NOT NULL",
      "email": "varchar(100) NOT NULL"
    },
    "Renter": {
      "*renter_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "phone": "varchar(20) NOT NULL",
      "email": "varchar(100) NOT NULL",
      "credit_score": "int()"
    },
    "Property": {
      "*property_id": "int() NOT NULL",
      "address": "varchar(200) NOT NULL",
      "price": "decimal(10,2) NOT NULL",
      "type": "varchar(50) NOT NULL",
      "status": "varchar(50) NOT NULL",
      "+agent_id": "int()",
      "+owner_id": "int()",
      "+renter_id": "int()"
    },
    "Owner": {
      "*owner_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "phone": "varchar(20) NOT NULL",
      "email": "varchar(100) NOT NULL"
    },
    "RentPayment": {
      "*payment_id": "int() NOT NULL",
      "amount": "decimal(10,2) NOT NULL",
      "payment_date": "date() NOT NULL",
      "month": "varchar(20) NOT NULL",
      "from_account": "varchar(50) NOT NULL",
      "to_account": "varchar(50) NOT NULL",
      "+renter_id": "int()",
      "+property_id": "int()"
    }
  },
  "relations": [
    "Agent:agent_id 1--* Property:agent_id",
    "Owner:owner_id 1--* Property:owner_id",
    "Renter:renter_id 1--* Property:renter_id",
    "RentPayment:renter_id *--1 Renter:renter_id",
    "RentPayment:property_id *--1 Property:property_id"
  ],
  "rankAdjustments": "",
  "label": "Real Estate Agency Database"
}