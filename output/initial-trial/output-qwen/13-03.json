{
  "tables": {
    "User": {
      "*username": "varchar(100) NOT NULL",
      "password": "varchar(100) NOT NULL"
    },
    "Agent": {
      "*agent_id": "int() NOT NULL",
      "+username": "varchar(100)",
      "role": "varchar(50)"
    },
    "PropertyOwner": {
      "*owner_id": "int() NOT NULL",
      "name": "varchar(200) NOT NULL",
      "contact_info": "varchar(200)"
    },
    "Property": {
      "*property_id": "int() NOT NULL",
      "+owner_id": "int()",
      "street": "varchar(200)",
      "number": "varchar(50)",
      "neighborhood": "varchar(200)",
      "city": "varchar(200)",
      "state": "varchar(100)",
      "zip_code": "varchar(50)",
      "last_maintenance_date": "date()"
    },
    "Renter": {
      "*renter_id": "int() NOT NULL",
      "name": "varchar(200) NOT NULL",
      "salary": "decimal(10,2)",
      "contact_info": "varchar(200)"
    },
    "Contract": {
      "*contract_id": "int() NOT NULL",
      "+property_id": "int()",
      "+renter_id": "int()",
      "+agent_id": "int()",
      "start_date": "date() NOT NULL",
      "end_date": "date() NOT NULL",
      "clauses": "text()",
      "deposit_refunded": "boolean()",
      "maintenance_flag": "boolean()"
    },
    "Payment": {
      "*payment_id": "int() NOT NULL",
      "+contract_id": "int()",
      "payment_date": "date() NOT NULL",
      "amount": "decimal(10,2) NOT NULL",
      "payment_method": "varchar(50) NOT NULL",
      "month": "varchar(20) NOT NULL",
      "payer_account": "varchar(100)",
      "receiver_account": "varchar(100)"
    },
    "Guarantor": {
      "*guarantor_id": "int() NOT NULL",
      "name": "varchar(200) NOT NULL",
      "contact_info": "varchar(200)",
      "percentage_covered": "decimal(5,2)"
    },
    "ContractGuarantor": {
      "*contract_guarantor_id": "int() NOT NULL",
      "+contract_id": "int()",
      "+guarantor_id": "int()"
    }
  },
  "relations": [
    "Agent:username 1--1 User:username",
    "Property:owner_id 1--* PropertyOwner:owner_id",
    "Contract:property_id 1--1 Property:property_id",
    "Contract:renter_id 1--* Renter:renter_id",
    "Contract:agent_id 1--1 Agent:agent_id",
    "Payment:contract_id 1--* Contract:contract_id",
    "ContractGuarantor:contract_id *--1 Contract:contract_id",
    "ContractGuarantor:guarantor_id *--1 Guarantor:guarantor_id"
  ],
  "rankAdjustments": "",
  "label": "Real Estate Agency Database"
}