{
  "tables": {
    "Agent": {
      "*agent_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)"
    },
    "Property": {
      "*property_id": "int() NOT NULL",
      "+agent_id": "int()",
      "address": "varchar(255) NOT NULL",
      "city": "varchar(100) NOT NULL",
      "state": "varchar(100) NOT NULL",
      "zip_code": "varchar(20) NOT NULL",
      "type": "varchar(50) NOT NULL",
      "price": "decimal(10,2) NOT NULL"
    },
    "Owner": {
      "*owner_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)"
    },
    "PropertyOwner": {
      "*property_owner_id": "int() NOT NULL",
      "+property_id": "int()",
      "+owner_id": "int()"
    },
    "Renter": {
      "*renter_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)"
    },
    "RentalAgreement": {
      "*agreement_id": "int() NOT NULL",
      "+renter_id": "int()",
      "+property_id": "int()",
      "start_date": "date() NOT NULL",
      "end_date": "date() NOT NULL",
      "clauses": "text()",
      "deposit_refunded": "boolean()"
    },
    "Payment": {
      "*payment_id": "int() NOT NULL",
      "+agreement_id": "int()",
      "payment_date": "date() NOT NULL",
      "amount": "decimal(10,2) NOT NULL",
      "month_paid_for": "varchar(20) NOT NULL",
      "payment_account": "varchar(50) NOT NULL",
      "receiving_account": "varchar(50) NOT NULL"
    }
  },
  "relations": [
    "Property:agent_id *--1 Agent:agent_id",
    "PropertyOwner:property_id *--1 Property:property_id",
    "PropertyOwner:owner_id *--1 Owner:owner_id",
    "RentalAgreement:renter_id *--1 Renter:renter_id",
    "RentalAgreement:property_id *--1 Property:property_id",
    "Payment:agreement_id *--1 RentalAgreement:agreement_id"
  ],
  "rankAdjustments": "",
  "label": "Real Estate Agency Database"
}