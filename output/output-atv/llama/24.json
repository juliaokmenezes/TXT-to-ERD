{
  "tables": {
    "Agent": {
      "*username": "varchar(100) NOT NULL",
      "password": "varchar(100) NOT NULL",
      "first_name": "varchar(100)",
      "last_name": "varchar(100)",
      "role": "varchar(100)"
    },
    "PropertyOwner": {
      "*owner_id": "int() NOT NULL",
      "first_name": "varchar(100)",
      "last_name": "varchar(100)"
    },
    "Renter": {
      "*renter_id": "int() NOT NULL",
      "first_name": "varchar(100)",
      "last_name": "varchar(100)",
      "salary": "decimal(10,2)"
    },
    "Guarantor": {
      "*guarantor_id": "int() NOT NULL",
      "first_name": "varchar(100)",
      "last_name": "varchar(100)"
    },
    "Property": {
      "*property_id": "int() NOT NULL",
      "+owner_id": "int()",
      "+agent_username": "varchar(100)"
    },
    "RentalAgreement": {
      "*agreement_id": "int() NOT NULL",
      "+property_id": "int()",
      "+renter_id": "int()",
      "start_date": "date() NOT NULL",
      "end_date": "date() NOT NULL",
      "clauses": "text()",
      "security_deposit_refund": "boolean()",
      "+guarantor_id": "int()"
    },
    "Payment": {
      "*payment_id": "int() NOT NULL",
      "+agreement_id": "int()",
      "payment_method": "varchar(100) NOT NULL",
      "payment_date": "date() NOT NULL",
      "payment_value": "decimal(10,2) NOT NULL",
      "receiving_account": "varchar(100) NOT NULL",
      "payment_account": "varchar(100) NOT NULL",
      "payment_month": "varchar(100) NOT NULL"
    },
    "Maintenance": {
      "*maintenance_id": "int() NOT NULL",
      "+property_id": "int()",
      "maintenance_date": "date() NOT NULL"
    }
  },
  "relations": [
    "Property:owner_id *--1 PropertyOwner:owner_id",
    "Property:agent_username *--1 Agent:username",
    "RentalAgreement:property_id *--1 Property:property_id",
    "RentalAgreement:renter_id *--1 Renter:renter_id",
    "RentalAgreement:guarantor_id *--1 Guarantor:guarantor_id",
    "Payment:agreement_id *--1 RentalAgreement:agreement_id",
    "Maintenance:property_id *--1 Property:property_id"
  ],
  "rankAdjustments": "",
  "label": "Real Estate Agency Database"
}