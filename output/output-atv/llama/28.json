{
  "tables": {
    "Agent": {
      "*agent_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100) NOT NULL",
      "phone": "varchar(20)"
    },
    "PropertyOwner": {
      "*owner_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100) NOT NULL",
      "phone": "varchar(20)"
    },
    "Renter": {
      "*renter_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100) NOT NULL",
      "phone": "varchar(20)"
    },
    "Property": {
      "*property_id": "int() NOT NULL",
      "+owner_id": "int()",
      "+agent_id": "int()"
    },
    "RentalAgreement": {
      "*agreement_id": "int() NOT NULL",
      "+property_id": "int()",
      "+renter_id": "int()",
      "start_date": "date() NOT NULL",
      "end_date": "date() NOT NULL",
      "clauses": "text()"
    },
    "Payment": {
      "*payment_id": "int() NOT NULL",
      "+agreement_id": "int()",
      "payment_method": "varchar(20) NOT NULL",
      "payment_date": "date() NOT NULL",
      "payment_amount": "decimal(10,2) NOT NULL",
      "receiving_account": "varchar(50) NOT NULL",
      "payment_account": "varchar(50) NOT NULL",
      "payment_month": "varchar(10) NOT NULL"
    },
    "SecurityDeposit": {
      "*deposit_id": "int() NOT NULL",
      "+agreement_id": "int()",
      "deposit_amount": "decimal(10,2) NOT NULL",
      "refund_status": "boolean() NOT NULL"
    },
    "MaintenanceRecord": {
      "*record_id": "int() NOT NULL",
      "+property_id": "int()",
      "date": "date() NOT NULL",
      "description": "varchar(200) NOT NULL",
      "cost": "decimal(10,2) NOT NULL",
      "contractor": "varchar(100) NOT NULL"
    }
  },
  "relations": [
    "Property:owner_id *--1 PropertyOwner:owner_id",
    "Property:agent_id *--1 Agent:agent_id",
    "RentalAgreement:property_id *--1 Property:property_id",
    "RentalAgreement:renter_id *--1 Renter:renter_id",
    "Payment:agreement_id *--1 RentalAgreement:agreement_id",
    "SecurityDeposit:agreement_id *--1 RentalAgreement:agreement_id",
    "MaintenanceRecord:property_id *--1 Property:property_id"
  ],
  "rankAdjustments": "",
  "label": "Real Estate Agency Database"
}