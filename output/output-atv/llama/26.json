{
  "tables": {
    "Agent": {
      "*username": "varchar(100) NOT NULL",
      "password": "varchar(100) NOT NULL",
      "first_name": "varchar(100)",
      "last_name": "varchar(100)",
      "email": "varchar(100)",
      "role": "varchar(100)"
    },
    "Owner": {
      "*owner_id": "int() NOT NULL",
      "first_name": "varchar(100)",
      "last_name": "varchar(100)",
      "email": "varchar(100)"
    },
    "Renter": {
      "*renter_id": "int() NOT NULL",
      "first_name": "varchar(100)",
      "last_name": "varchar(100)",
      "email": "varchar(100)"
    },
    "Property": {
      "*property_id": "int() NOT NULL",
      "+owner_id": "int()",
      "+agent_username": "varchar(100)"
    },
    "RentalAgreement": {
      "*agreement_id": "int() NOT NULL",
      "+property_id": "int()",
      "+renter_id": "int()",
      "start_date": "date() NOT NULL",
      "end_date": "date() NOT NULL",
      "clauses": "text()"
    },
    "Payment": {
      "*payment_id": "int() NOT NULL",
      "+agreement_id": "int()",
      "payment_method": "varchar(100) NOT NULL",
      "payment_date": "date() NOT NULL",
      "amount": "decimal(10,2) NOT NULL",
      "payer_account": "varchar(100) NOT NULL",
      "receiver_account": "varchar(100) NOT NULL",
      "month": "varchar(100) NOT NULL"
    },
    "Maintenance": {
      "*maintenance_id": "int() NOT NULL",
      "+property_id": "int()",
      "maintenance_date": "date() NOT NULL"
    },
    "Rating": {
      "*rating_id": "int() NOT NULL",
      "rater_type": "varchar(100) NOT NULL",
      "rated_type": "varchar(100) NOT NULL",
      "rater_id": "int() NOT NULL",
      "rated_id": "int() NOT NULL",
      "rate": "int() NOT NULL"
    }
  },
  "relations": [
    "Property:owner_id *--1 Owner:owner_id",
    "Property:agent_username *--1 Agent:username",
    "RentalAgreement:property_id *--1 Property:property_id",
    "RentalAgreement:renter_id *--1 Renter:renter_id",
    "Payment:agreement_id *--1 RentalAgreement:agreement_id",
    "Maintenance:property_id *--1 Property:property_id",
    "Rating:rater_id *--1 Agent:username",
    "Rating:rater_id *--1 Owner:owner_id",
    "Rating:rater_id *--1 Renter:renter_id",
    "Rating:rated_id *--1 Agent:username",
    "Rating:rated_id *--1 Owner:owner_id",
    "Rating:rated_id *--1 Renter:renter_id"
  ],
  "rankAdjustments": "",
  "label": "Real Estate Agency Database"
}