{
  "tables": {
    "Agent": {
      "*agent_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "agreements_signed": "int()"
    },
    "PropertyOwner": {
      "*owner_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL"
    },
    "Property": {
      "*property_id": "int() NOT NULL",
      "+owner_id": "int()",
      "+agent_id": "int()"
    },
    "Renter": {
      "*renter_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL"
    },
    "Rental": {
      "*rental_id": "bigserial() NOT NULL",
      "+property_id": "int()",
      "+renter_id": "int()"
    },
    "Payment": {
      "*payment_id": "bigserial() NOT NULL",
      "+rental_id": "bigint()",
      "payment_date": "date() NOT NULL",
      "payment_amount": "decimal(10,2) NOT NULL",
      "payment_account": "varchar(100) NOT NULL",
      "receiving_account": "varchar(100) NOT NULL",
      "payment_month": "varchar(10) NOT NULL",
      "passed_to_owner": "boolean() NOT NULL"
    }
  },
  "relations": [
    "Property:owner_id *--1 PropertyOwner:owner_id",
    "Property:agent_id *--1 Agent:agent_id",
    "Rental:property_id *--1 Property:property_id",
    "Rental:renter_id *--1 Renter:renter_id",
    "Payment:rental_id *--1 Rental:rental_id"
  ],
  "rankAdjustments": "",
  "label": "Real Estate Agency Database"
}