{
  "tables": {
    "Agent": {
      "*agent_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)"
    },
    "Owner": {
      "*owner_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)"
    },
    "Renter": {
      "*renter_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)"
    },
    "Property": {
      "*property_id": "int() NOT NULL",
      "address": "varchar(200) NOT NULL",
      "price": "decimal(10,2) NOT NULL",
      "+owner_id": "int()",
      "+agent_id": "int()"
    },
    "Lease": {
      "*lease_id": "int() NOT NULL",
      "+property_id": "int()",
      "+renter_id": "int()",
      "start_date": "date() NOT NULL",
      "end_date": "date() NOT NULL"
    },
    "Payment": {
      "*payment_id": "int() NOT NULL",
      "+lease_id": "int()",
      "amount": "decimal(10,2) NOT NULL",
      "payment_date": "date() NOT NULL",
      "month": "varchar(20) NOT NULL",
      "currency": "varchar(10) NOT NULL"
    }
  },
  "relations": [
    "Property:owner_id 1--1 Owner:owner_id",
    "Property:agent_id 1--* Agent:agent_id",
    "Lease:property_id 1--1 Property:property_id",
    "Lease:renter_id 1--* Renter:renter_id",
    "Payment:lease_id 1--1 Lease:lease_id"
  ],
  "rankAdjustments": "",
  "label": "Real Estate Agency Database"
}