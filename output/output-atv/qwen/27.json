{
  "tables": {
    "Agent": {
      "*agent_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)"
    },
    "Owner": {
      "*owner_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)"
    },
    "Property": {
      "*property_id": "int() NOT NULL",
      "address": "varchar(255) NOT NULL",
      "price": "decimal(10,2) NOT NULL",
      "+owner_id": "int()",
      "+agent_id": "int()"
    },
    "Renter": {
      "*renter_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)"
    },
    "RentalAgreement": {
      "*agreement_id": "int() NOT NULL",
      "+property_id": "int()",
      "+renter_id": "int()",
      "start_date": "date() NOT NULL",
      "end_date": "date() NOT NULL",
      "clauses": "text()",
      "deposit_refunded": "boolean()"
    },
    "Payment": {
      "*payment_id": "int() NOT NULL",
      "+agreement_id": "int()",
      "payment_date": "date() NOT NULL",
      "amount": "decimal(10,2) NOT NULL",
      "payment_method": "varchar(50) NOT NULL",
      "month_paid_for": "varchar(20) NOT NULL",
      "sender_account": "varchar(50)",
      "receiver_account": "varchar(50)"
    },
    "Insurance": {
      "*policy_id": "int() NOT NULL",
      "+agreement_id": "int()",
      "policy_number": "varchar(100) NOT NULL",
      "coverage_amount": "decimal(10,2) NOT NULL",
      "provider": "varchar(100) NOT NULL"
    }
  },
  "relations": [
    "Property:owner_id 1--* Owner:owner_id",
    "Property:agent_id 1--1 Agent:agent_id",
    "RentalAgreement:property_id 1--1 Property:property_id",
    "RentalAgreement:renter_id 1--* Renter:renter_id",
    "Payment:agreement_id 1--* RentalAgreement:agreement_id",
    "Insurance:agreement_id 1--* RentalAgreement:agreement_id"
  ],
  "rankAdjustments": "",
  "label": "Real Estate Management System"
}