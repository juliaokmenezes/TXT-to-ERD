{
  "tables": {
    "Agent": {
      "*agentID": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)"
    },
    "Property": {
      "*propertyID": "int() NOT NULL",
      "address": "varchar(255) NOT NULL",
      "type": "varchar(50)",
      "lastMaintenanceDate": "date()",
      "ownerID": "int()"
    },
    "Owner": {
      "*ownerID": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)"
    },
    "Tenant": {
      "*tenantID": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)",
      "salary": "decimal(10,2)"
    },
    "Contract": {
      "*contractID": "int() NOT NULL",
      "startDate": "date() NOT NULL",
      "endDate": "date() NOT NULL",
      "clauses": "text",
      "depositRefunded": "boolean",
      "tenantID": "int()",
      "propertyID": "int()",
      "agentID": "int()"
    },
    "Payment": {
      "*paymentID": "int() NOT NULL",
      "paymentDate": "date() NOT NULL",
      "amount": "decimal(10,2) NOT NULL",
      "month": "int() NOT NULL",
      "tenantID": "int()",
      "contractID": "int()"
    },
    "Guarantor": {
      "*guarantorID": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)",
      "tenantID": "int()"
    }
  },
  "relations": [
    "Agent:agentID 1--* Contract:agentID",
    "Property:propertyID 1--* Contract:propertyID",
    "Owner:ownerID 1--* Property:ownerID",
    "Tenant:tenantID 1--* Contract:tenantID",
    "Tenant:tenantID 1--* Payment:tenantID",
    "Contract:contractID 1--* Payment:contractID",
    "Tenant:tenantID 1--* Guarantor:tenantID"
  ],
  "rankAdjustments": [],
  "label": "Real Estate Agency Database Schema"
}