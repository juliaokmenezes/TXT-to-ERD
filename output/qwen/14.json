{
  "tables": {
    "ImobiliariaAgent": {
      "*agent_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)"
    },
    "Owner": {
      "*owner_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)"
    },
    "Tenant": {
      "*tenant_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)",
      "verified_income": "decimal(10,2)"
    },
    "Property": {
      "*property_id": "int() NOT NULL",
      "address": "varchar(200) NOT NULL",
      "price": "decimal(10,2) NOT NULL",
      "status": "varchar(50)",
      "+owner_id": "int()",
      "+agent_id": "int()"
    },
    "LeaseContract": {
      "*contract_id": "int() NOT NULL",
      "+tenant_id": "int()",
      "+property_id": "int()",
      "start_date": "date() NOT NULL",
      "end_date": "date() NOT NULL",
      "rent_amount": "decimal(10,2) NOT NULL",
      "+surety_id": "int()"
    },
    "Payment": {
      "*payment_id": "int() NOT NULL",
      "+contract_id": "int()",
      "payment_date": "date() NOT NULL",
      "amount": "decimal(10,2) NOT NULL"
    },
    "Surety": {
      "*surety_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)"
    },
    "Maintenance": {
      "*maintenance_id": "int() NOT NULL",
      "+property_id": "int()",
      "maintenance_date": "date() NOT NULL",
      "description": "text()"
    },
    "AgentReferral": {
      "*referral_id": "int() NOT NULL",
      "+referring_agent_id": "int()",
      "+referred_agent_id": "int()",
      "referral_date": "date() NOT NULL",
      "commission": "decimal(10,2)"
    },
    "DamageFine": {
      "*fine_id": "int() NOT NULL",
      "+property_id": "int()",
      "+tenant_id": "int()",
      "fine_date": "date() NOT NULL",
      "reason": "text() NOT NULL",
      "evidence": "text()"
    }
  },
  "relations": [
    "ImobiliariaAgent:agent_id 1--* Property:agent_id",
    "Owner:owner_id 1--* Property:owner_id",
    "Tenant:tenant_id 1--* LeaseContract:tenant_id",
    "Property:property_id 1--* LeaseContract:property_id",
    "LeaseContract:contract_id 1--* Payment:contract_id",
    "Surety:surety_id 1--* LeaseContract:surety_id",
    "Property:property_id 1--* Maintenance:property_id",
    "ImobiliariaAgent:agent_id 1--* AgentReferral:referring_agent_id",
    "ImobiliariaAgent:agent_id 1--* AgentReferral:referred_agent_id",
    "Property:property_id 1--* DamageFine:property_id",
    "Tenant:tenant_id 1--* DamageFine:tenant_id"
  ],
  "rankAdjustments": [],
  "label": "Imobili\u00e1ria Gest\u00e3o"
}