{
  "tables": {
    "Agent": {
      "*agent_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)"
    },
    "Owner": {
      "*owner_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)"
    },
    "Tenant": {
      "*tenant_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)",
      "monthly_income": "decimal(10,2)"
    },
    "Property": {
      "*property_id": "int() NOT NULL",
      "address": "varchar(255) NOT NULL",
      "type": "varchar(50) NOT NULL",
      "+owner_id": "int()",
      "+agent_id": "int()"
    },
    "Lease": {
      "*lease_id": "int() NOT NULL",
      "+tenant_id": "int()",
      "+property_id": "int()",
      "start_date": "date() NOT NULL",
      "end_date": "date() NOT NULL",
      "+surety_id": "int()"
    },
    "Payment": {
      "*payment_id": "int() NOT NULL",
      "+lease_id": "int()",
      "amount": "decimal(10,2) NOT NULL",
      "payment_date": "date() NOT NULL"
    },
    "Maintenance": {
      "*maintenance_id": "int() NOT NULL",
      "+property_id": "int()",
      "maintenance_date": "date() NOT NULL",
      "description": "text()"
    },
    "Surety": {
      "*surety_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)"
    }
  },
  "relations": [
    "Agent:agent_id 1--* Property:agent_id",
    "Owner:owner_id 1--* Property:owner_id",
    "Tenant:tenant_id 1--* Lease:tenant_id",
    "Property:property_id 1--* Lease:property_id",
    "Lease:lease_id 1--* Payment:lease_id",
    "Property:property_id 1--* Maintenance:property_id",
    "Surety:surety_id 1--* Lease:surety_id"
  ],
  "rankAdjustments": "",
  "label": "Imobili\u00e1ria Gest\u00e3o"
}