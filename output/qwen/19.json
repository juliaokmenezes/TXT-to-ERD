{
  "tables": {
    "Agent": {
      "*agent_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100) NOT NULL",
      "phone": "varchar(20)"
    },
    "Owner": {
      "*owner_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100) NOT NULL",
      "phone": "varchar(20)"
    },
    "Tenant": {
      "*tenant_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100) NOT NULL",
      "phone": "varchar(20)"
    },
    "Property": {
      "*property_id": "int() NOT NULL",
      "address": "varchar(200) NOT NULL",
      "city": "varchar(100) NOT NULL",
      "state": "varchar(100) NOT NULL",
      "zip_code": "varchar(20) NOT NULL",
      "+owner_id": "int()",
      "+agent_id": "int()"
    },
    "Lease": {
      "*lease_id": "int() NOT NULL",
      "+tenant_id": "int()",
      "+property_id": "int()",
      "start_date": "date() NOT NULL",
      "end_date": "date() NOT NULL",
      "rent_amount": "decimal(10,2) NOT NULL"
    },
    "Payment": {
      "*payment_id": "int() NOT NULL",
      "+lease_id": "int()",
      "payment_date": "date() NOT NULL",
      "amount": "decimal(10,2) NOT NULL",
      "payment_method": "varchar(50) NOT NULL"
    },
    "Maintenance": {
      "*maintenance_id": "int() NOT NULL",
      "+property_id": "int()",
      "maintenance_date": "date() NOT NULL",
      "description": "text() NOT NULL"
    },
    "Extension": {
      "*extension_id": "int() NOT NULL",
      "+lease_id": "int()",
      "extension_start_date": "date() NOT NULL",
      "extension_end_date": "date() NOT NULL",
      "new_rent_amount": "decimal(10,2) NOT NULL"
    }
  },
  "relations": [
    "Agent:agent_id 1--* Property:agent_id",
    "Owner:owner_id 1--* Property:owner_id",
    "Tenant:tenant_id 1--* Lease:tenant_id",
    "Property:property_id 1--* Lease:property_id",
    "Lease:lease_id 1--* Payment:lease_id",
    "Property:property_id 1--* Maintenance:property_id",
    "Lease:lease_id 1--* Extension:lease_id"
  ],
  "rankAdjustments": [],
  "label": "Imobiliaria Sistema"
}