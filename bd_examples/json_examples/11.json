{
    "tables": {
        "RealEstateAgent": {
            "*id_realEstate": "int() NOT NULL",
            "name": "varchar(100) NOT NULL",
            "phoneNumber": "char(20) NOT NULL",
            "email": "varchar(50) NOT NULL"
        },
        "Owner": {
            "*id_owner": "int() NOT NULL",
            "name": "varchar(100) NOT NULL",
            "phoneNumber": "char(20) NOT NULL",
            "email": "varchar(50) NOT NULL"
        },
        "Renter": {
            "*id_renter": "int() NOT NULL",
            "name": "varchar(100) NOT NULL",
            "phoneNumber": "char(20) NOT NULL",
            "email": "varchar(50) NOT NULL"
        },
        "Guarantor": {
            "*id_guarantor": "int() NOT NULL",
            "name": "varchar(100) NOT NULL",
            "phoneNumber": "char(20) NOT NULL",
            "email": "varchar(50) NOT NULL"
        },
        "Property": {
            "*id_property": "int() NOT NULL",
            "address": "varchar(200) NOT NULL",
            "rentValue": "decimal(10,2) NOT NULL",
            "+id_owner": "int() NOT NULL",
            "+id_realEstate": "int() NOT NULL",
            "lastMaintenanceDate": "date() NOT NULL"
        },
        "Rents": {
            "*id_rent": "int() NOT NULL",
            "+id_property": "int() NOT NULL",
            "+id_renter": "int() NOT NULL",
            "rentInitialDate": "date() NOT NULL",
            "rentFinalDate": "date()",
            "activeContract": "boolean NOT NULL",
            "monthlyRent": "decimal(10,2) NOT NULL",
            "depositAmount": "decimal(10,2) NOT NULL"
        },
        "Payments": {
            "*id_payment": "int() NOT NULL",
            "+id_rent": "int() NOT NULL",
            "monthReference": "int() NOT NULL",
            "yearReference": "int() NOT NULL",
            "amountPaid": "decimal(10,2) NOT NULL",
            "paymentAccount": "varchar(50) NOT NULL",
            "receivingAccount": "varchar(50) NOT NULL",
            "paymentDate": "date() NOT NULL",
            "paymentMethod": "enum('Boleto', 'PIX', 'Credit Card', 'Bank Transfer')"
        },
        "Agreement": {
            "*id_agreement": "int() NOT NULL",
            "+id_rent": "int() NOT NULL",
            "agreementStartDate": "date() NOT NULL",
            "agreementEndDate": "date() NOT NULL",
            "clauses": "text NOT NULL",
            "depositRefunded": "boolean NOT NULL",
            "maintenanceOutdated": "boolean NOT NULL"
        },
        "AgreementGuarantors": {
            "*id_agreementGuarantor": "int() NOT NULL",
            "+id_agreement": "int() NOT NULL",
            "+id_guarantor": "int() NOT NULL",
            "percentageCovered": "decimal(5,2) NOT NULL"
        }
    },
    "relations": [
        "Owner:id_owner 1--* Property:id_owner",
        "RealEstateAgent:id_realEstate 1--* Property:id_realEstate",
        "Property:id_property 1--* Rents:id_property",
        "Renter:id_renter 1--* Rents:id_renter",
        "Rents:id_rent 1--* Payments:id_rent",
        "Rents:id_rent 1--1 Agreement:id_rent",
        "Agreement:id_agreement 1--* AgreementGuarantors:id_agreement",
        "Guarantor:id_guarantor 1--* AgreementGuarantors:id_guarantor"
    ],
    "rankAdjustments": [],
    "label": ""
}

