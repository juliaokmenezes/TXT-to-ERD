{
    "tables": {
        "RealEstateAgent": {
            "*id_realEstate": "int() NOT NULL",
            "name": "varchar(100) NOT NULL",
            "phoneNumber": "char(20) NOT NULL",
            "email": "varchar(50) NOT NULL"
        },
        "Owner": {
            "*id_owner": "int() NOT NULL",
            "name": "varchar(100) NOT NULL",
            "phoneNumber": "char(20) NOT NULL",
            "email": "varchar(50) NOT NULL"
        },
        "Renter": {
            "*id_renter": "int() NOT NULL",
            "name": "varchar(100) NOT NULL",
            "phoneNumber": "char(20) NOT NULL",
            "salario": "decimal(10,2) NOT NULL",
            "email": "varchar(50) NOT NULL"
        },
        "Property": {
            "*id_property": "int() NOT NULL",
            "address": "varchar(200) NOT NULL",
            "rentValue": "decimal(10,2) NOT NULL",
            "+id_owner": "int() NOT NULL",
            "+id_realEstate": "int() NOT NULL",
            "disponivelParaAluguel": "boolean() NOT NULL"
        },
        "Rents": {
            "*id_rent": "int() NOT NULL",
            "+id_property": "int() NOT NULL",
            "+id_renter": "int() NOT NULL",
            "+id_inspection": "int() NOT NULL",
            "rentInitialDate": "date() NOT NULL",
            "rentFinalDate": "date()",
            "depositAmount": "decimal(10,2) NOT NULL"
        },
        "Payments": {
            "*id_payment": "int() NOT NULL",
            "+id_rent": "int() NOT NULL",
            "valorAluguel": "decimal(10,2) NOT NULL",
            "paymentAccount": "varchar(50) NOT NULL",
            "receivingAccount": "varchar(50) NOT NULL",
            "paymentDate": "date() NOT NULL",
            "paymentMethod": "enum('Boleto', 'PIX', 'Cartão de Crédito', 'Transferência Bancária') NOT NULL"
        },
        "Fiador": {
            "*id_fiador": "int() NOT NULL",
            "+id_rent": "int() NOT NULL"
        },
        "Maintenance": {
            "*id_maintenance": "int() NOT NULL",
            "+id_property": "int() NOT NULL",
            "lastMaintenanceDate": "date() NOT NULL",
            "description": "text()"
        },
        "Inspections": {
            "*id_inspection": "int() NOT NULL",
            "+id_property": "int() NOT NULL",
            "inspectionDate": "date() NOT NULL",
            "damageReport": "text()",
            "repairsNeeded": "boolean() NOT NULL"
        },
        "AgentReferrals": {
            "*id_referral": "int() NOT NULL",
            "+id_referringAgent": "int() NOT NULL",
            "+id_referredAgent": "int() NOT NULL",
            "referralDate": "date() NOT NULL",
            "commissionEarned": "decimal(10,2)"
        }
    },

    "relations": [
        "Property:id_realEstate 1--* RealEstateAgent:id_realEstate",
        "Property:id_owner 1--* Owner:id_owner",
        "Property:id_property *--1 Rents:id_property",
        "Renter:id_renter *--1 Rents:id_renter",
        "Rents:id_rent *--1 Payments:id_rent",
        "Rents:id_rent 1--* Fiador:id_rent",
        "Maintenance:id_property 1--* Property:id_property",
        "Inspections:id_property 1--* Property:id_property",
        "Rents:id_inspection 1--1 Inspections:id_inspection",
        "AgentReferrals:id_referringAgent 1--* RealEstateAgent:id_realEstate",
        "AgentReferrals:id_referredAgent 1--* RealEstateAgent:id_realEstate"
    ],
    "rankAdjustments": "",
    "label": "15"
}
