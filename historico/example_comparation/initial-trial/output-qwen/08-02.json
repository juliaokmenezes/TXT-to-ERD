{
  "tables": {
    "Agent": {
      "*agent_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)"
    },
    "PropertyOwner": {
      "*owner_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)"
    },
    "Property": {
      "*property_id": "int() NOT NULL",
      "address": "varchar(200) NOT NULL",
      "price": "decimal(10,2) NOT NULL",
      "last_maintenance_date": "date() NOT NULL",
      "+agent_id": "int()",
      "+owner_id": "int()"
    },
    "Renter": {
      "*renter_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)",
      "salary": "decimal(10,2) NOT NULL"
    },
    "Contract": {
      "*contract_id": "int() NOT NULL",
      "start_date": "date() NOT NULL",
      "end_date": "date() NOT NULL",
      "clauses": "text()",
      "deposit_refunded": "boolean() NOT NULL",
      "guarantor_count": "int() NOT NULL",
      "+renter_id": "int()",
      "+property_id": "int()"
    },
    "Payment": {
      "*payment_id": "int() NOT NULL",
      "amount": "decimal(10,2) NOT NULL",
      "payment_date": "date() NOT NULL",
      "month": "varchar(20) NOT NULL",
      "+contract_id": "int()"
    },
    "Guarantor": {
      "*guarantor_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)",
      "percentage_covered": "decimal(5,2) NOT NULL",
      "+contract_id": "int()"
    }
  },
  "relations": [
    "Property:agent_id *--1 Agent:agent_id",
    "Property:owner_id *--1 PropertyOwner:owner_id",
    "Contract:renter_id *--1 Renter:renter_id",
    "Contract:property_id *--1 Property:property_id",
    "Payment:contract_id *--1 Contract:contract_id",
    "Guarantor:contract_id *--1 Contract:contract_id"
  ],
  "rankAdjustments": "",
  "label": "Real Estate Agency Database"
}