{
  "tables": {
    "Agent": {
      "*agent_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "phone": "varchar(20)",
      "email": "varchar(100)"
    },
    "PropertyOwner": {
      "*owner_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "phone": "varchar(20)",
      "email": "varchar(100)"
    },
    "Property": {
      "*property_id": "int() NOT NULL",
      "address": "varchar(200) NOT NULL",
      "price": "decimal(10,2) NOT NULL",
      "status": "varchar(50) NOT NULL",
      "+agent_id": "int()",
      "+owner_id": "int()"
    },
    "Renter": {
      "*renter_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "phone": "varchar(20)",
      "email": "varchar(100)",
      "+current_rental_id": "int()"
    },
    "Rental": {
      "*rental_id": "int() NOT NULL",
      "start_date": "date() NOT NULL",
      "end_date": "date()",
      "+renter_id": "int() NOT NULL",
      "+property_id": "int() NOT NULL"
    }
  },
  "relations": [
    "Agent:agent_id 1--* Property:agent_id",
    "PropertyOwner:owner_id 1--* Property:owner_id",
    "Renter:renter_id *--1 Rental:rental_id",
    "Property:property_id *--1 Rental:property_id",
    "Renter:current_rental_id *--1 Rental:rental_id"
  ],
  "rankAdjustments": "",
  "label": "Real Estate Agency Database"
}