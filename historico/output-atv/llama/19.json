{
  "tables": {
    "Agent": {
      "*agent_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "phone": "varchar(20) NOT NULL",
      "email": "varchar(100) NOT NULL"
    },
    "Renter": {
      "*renter_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "salary": "decimal(10,2) NOT NULL"
    },
    "Property": {
      "*property_id": "int() NOT NULL",
      "+owner_id": "int()",
      "+agent_id": "int()",
      "address": "varchar(200) NOT NULL"
    },
    "Owner": {
      "*owner_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "phone": "varchar(20) NOT NULL",
      "email": "varchar(100) NOT NULL"
    },
    "RentalAgreement": {
      "*agreement_id": "int() NOT NULL",
      "+property_id": "int()",
      "+renter_id": "int()",
      "start_date": "date() NOT NULL",
      "end_date": "date() NOT NULL",
      "clauses": "text()",
      "security_deposit_refund": "boolean() NOT NULL"
    },
    "Payment": {
      "*payment_id": "int() NOT NULL",
      "+agreement_id": "int()",
      "payment_date": "date() NOT NULL",
      "payment_amount": "decimal(10,2) NOT NULL",
      "payment_account": "varchar(100) NOT NULL",
      "receiving_account": "varchar(100) NOT NULL"
    },
    "Guarantor": {
      "*guarantor_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL"
    },
    "Guarantee": {
      "*guarantee_id": "int() NOT NULL",
      "+agreement_id": "int()",
      "+guarantor_id": "int()"
    },
    "Maintenance": {
      "*maintenance_id": "int() NOT NULL",
      "+property_id": "int()",
      "maintenance_date": "date() NOT NULL"
    },
    "Referral": {
      "*referral_id": "int() NOT NULL",
      "+referred_agent_id": "int()",
      "+referring_agent_id": "int()",
      "referral_date": "date() NOT NULL",
      "commission": "decimal(10,2) NOT NULL"
    }
  },
  "relations": [
    "Property:owner_id *--1 Owner:owner_id",
    "Property:agent_id *--1 Agent:agent_id",
    "RentalAgreement:property_id *--1 Property:property_id",
    "RentalAgreement:renter_id *--1 Renter:renter_id",
    "Payment:agreement_id *--1 RentalAgreement:agreement_id",
    "Guarantee:agreement_id *--1 RentalAgreement:agreement_id",
    "Guarantee:guarantor_id *--1 Guarantor:guarantor_id",
    "Maintenance:property_id *--1 Property:property_id",
    "Referral:referred_agent_id *--1 Agent:agent_id",
    "Referral:referring_agent_id *--1 Agent:agent_id"
  ],
  "rankAdjustments": "",
  "label": "Real Estate Agency Database"
}