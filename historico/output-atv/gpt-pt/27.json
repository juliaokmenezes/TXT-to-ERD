{
  "tables": {
    "Agente": {
      "*id_agente": "int() NOT NULL",
      "nome": "varchar(200) NOT NULL",
      "telefone": "varchar(15)"
    },
    "Proprietario": {
      "*id_proprietario": "int() NOT NULL",
      "nome": "varchar(200) NOT NULL",
      "telefone": "varchar(15)"
    },
    "Locatario": {
      "*id_locatario": "int() NOT NULL",
      "nome": "varchar(200) NOT NULL",
      "telefone": "varchar(15)"
    },
    "Propriedade": {
      "*id_propriedade": "int() NOT NULL",
      "+id_agente": "int()",
      "+id_proprietario": "int()",
      "+id_locatario": "int()",
      "endereco": "varchar(500) NOT NULL",
      "valor_aluguel": "decimal(10,2) NOT NULL"
    },
    "Contrato": {
      "*id_contrato": "int() NOT NULL",
      "+id_propriedade": "int()",
      "data_inicio": "date() NOT NULL",
      "data_termino": "date()",
      "clausulas": "text()",
      "deposito_reembolsado": "boolean()"
    },
    "Pagamento": {
      "*id_pagamento": "int() NOT NULL",
      "+id_contrato": "int()",
      "mes_referente": "varchar(20) NOT NULL",
      "valor": "decimal(10,2) NOT NULL",
      "metodo_pagamento": "varchar(50) NOT NULL"
    },
    "Seguro": {
      "*id_seguro": "int() NOT NULL",
      "+id_contrato": "int()",
      "numero_apolice": "varchar(100) NOT NULL",
      "valor_cobertura": "decimal(10,2) NOT NULL",
      "provedor": "varchar(200) NOT NULL"
    }
  },
  "relations": [
    "Propriedade:id_agente 1--* Agente:id_agente",
    "Propriedade:id_proprietario 1--* Proprietario:id_proprietario",
    "Propriedade:id_locatario ?--1 Locatario:id_locatario",
    "Contrato:id_propriedade 1--1 Propriedade:id_propriedade",
    "Pagamento:id_contrato 1--* Contrato:id_contrato",
    "Seguro:id_contrato 1--1 Contrato:id_contrato"
  ],
  "rankAdjustments": "",
  "label": "Sistema de Gest\u00e3o de Propriedades Alugadas"
}