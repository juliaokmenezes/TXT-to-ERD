{
  "tables": {
    "Agent": {
      "*agent_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(15)"
    },
    "Owner": {
      "*owner_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(15)"
    },
    "Renter": {
      "*renter_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(15)"
    },
    "Property": {
      "*property_id": "int() NOT NULL",
      "+agent_id": "int()",
      "+owner_id": "int()",
      "+renter_id": "int()",
      "address": "varchar(200) NOT NULL",
      "rent_amount": "decimal(10,2) NOT NULL"
    },
    "Payment": {
      "*payment_id": "int() NOT NULL",
      "+property_id": "int()",
      "payment_date": "date() NOT NULL",
      "amount": "decimal(10,2) NOT NULL",
      "payment_account": "varchar(100) NOT NULL",
      "receiving_account": "varchar(100) NOT NULL",
      "payment_month": "varchar(20) NOT NULL",
      "currency": "varchar(10) NOT NULL"
    }
  },
  "relations": [
    "Property:agent_id *--1 Agent:agent_id",
    "Property:owner_id *--1 Owner:owner_id",
    "Property:renter_id ?--1 Renter:renter_id",
    "Payment:property_id *--1 Property:property_id"
  ],
  "rankAdjustments": "",
  "label": "Real Estate Agency Management System"
}