{
  "tables": {
    "Agent": {
      "*agent_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(15)"
    },
    "Owner": {
      "*owner_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(15)"
    },
    "Renter": {
      "*renter_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(15)"
    },
    "Property": {
      "*property_id": "int() NOT NULL",
      "+agent_id": "int()",
      "+owner_id": "int()",
      "address": "varchar(200) NOT NULL",
      "city": "varchar(100)",
      "state": "varchar(100)",
      "zip_code": "varchar(10)"
    },
    "RentalAgreement": {
      "*agreement_id": "int() NOT NULL",
      "+property_id": "int()",
      "+renter_id": "int()",
      "start_date": "date() NOT NULL",
      "end_date": "date()",
      "clauses": "text()",
      "security_deposit_refunded": "boolean()"
    },
    "Payment": {
      "*payment_id": "int() NOT NULL",
      "+agreement_id": "int()",
      "payment_date": "date() NOT NULL",
      "amount": "decimal(10,2) NOT NULL",
      "payment_method": "varchar(50) NOT NULL",
      "payment_account": "varchar(100)",
      "receiving_account": "varchar(100)",
      "payment_month": "varchar(20) NOT NULL"
    },
    "Maintenance": {
      "*maintenance_id": "int() NOT NULL",
      "+property_id": "int()",
      "date": "date() NOT NULL",
      "description": "text()",
      "cost": "decimal(10,2)",
      "contractor": "varchar(100)"
    },
    "LeaseExtension": {
      "*extension_id": "int() NOT NULL",
      "+agreement_id": "int()",
      "old_rent_value": "decimal(10,2)",
      "new_rent_value": "decimal(10,2)",
      "extension_date": "date() NOT NULL"
    }
  },
  "relations": [
    "Property:agent_id *--1 Agent:agent_id",
    "Property:owner_id *--1 Owner:owner_id",
    "RentalAgreement:property_id 1--1 Property:property_id",
    "RentalAgreement:renter_id *--1 Renter:renter_id",
    "Payment:agreement_id *--1 RentalAgreement:agreement_id",
    "Maintenance:property_id *--1 Property:property_id",
    "LeaseExtension:agreement_id *--1 RentalAgreement:agreement_id"
  ],
  "rankAdjustments": "",
  "label": "Real Estate Agency Management System"
}