{
  "tables": {
    "Agent": {
      "*agent_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(15)",
      "user_account": "varchar(100) NOT NULL",
      "password": "varchar(100) NOT NULL",
      "role": "varchar(50) NOT NULL"
    },
    "Owner": {
      "*owner_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(15)"
    },
    "Renter": {
      "*renter_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(15)",
      "salary": "decimal(10,2)"
    },
    "Guarantor": {
      "*guarantor_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(15)"
    },
    "Property": {
      "*property_id": "int() NOT NULL",
      "+owner_id": "int()",
      "+agent_id": "int()",
      "address": "varchar(200) NOT NULL",
      "last_maintenance_date": "date()"
    },
    "RentalAgreement": {
      "*agreement_id": "int() NOT NULL",
      "+property_id": "int()",
      "+renter_id": "int()",
      "+guarantor_id": "int()",
      "start_date": "date() NOT NULL",
      "end_date": "date()",
      "clauses": "text()",
      "security_deposit_refunded": "boolean()"
    },
    "Payment": {
      "*payment_id": "int() NOT NULL",
      "+agreement_id": "int()",
      "payment_date": "date() NOT NULL",
      "amount": "decimal(10,2) NOT NULL",
      "payment_method": "varchar(50) NOT NULL",
      "payment_account": "varchar(100)",
      "receiving_account": "varchar(100)",
      "payment_month": "varchar(20) NOT NULL"
    },
    "RentHistory": {
      "*history_id": "int() NOT NULL",
      "+property_id": "int()",
      "rent_price": "decimal(10,2) NOT NULL",
      "date_recorded": "date() NOT NULL"
    }
  },
  "relations": [
    "Property:owner_id 1--* Owner:owner_id",
    "Property:agent_id 1--* Agent:agent_id",
    "RentalAgreement:property_id 1--1 Property:property_id",
    "RentalAgreement:renter_id 1--1 Renter:renter_id",
    "RentalAgreement:guarantor_id ?--* Guarantor:guarantor_id",
    "Payment:agreement_id 1--1 RentalAgreement:agreement_id",
    "RentHistory:property_id 1--* Property:property_id"
  ],
  "rankAdjustments": "",
  "label": "Real Estate Agency Management System"
}