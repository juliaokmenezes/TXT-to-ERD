{
  "tables": {
    "Agent": {
      "*agent_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(15)"
    },
    "Owner": {
      "*owner_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(15)"
    },
    "Renter": {
      "*renter_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(15)"
    },
    "Property": {
      "*property_id": "int() NOT NULL",
      "+agent_id": "int()",
      "+owner_id": "int()",
      "+renter_id": "int()",
      "address": "varchar(200) NOT NULL",
      "city": "varchar(100)",
      "state": "varchar(100)",
      "zip_code": "varchar(10)"
    },
    "Payment": {
      "*payment_id": "int() NOT NULL",
      "+property_id": "int()",
      "amount": "decimal(10,2) NOT NULL",
      "payment_date": "date() NOT NULL",
      "payment_method": "varchar(50) NOT NULL",
      "month_paid_for": "varchar(20) NOT NULL",
      "payer_account": "varchar(100)",
      "receiver_account": "varchar(100)"
    },
    "RentalAgreement": {
      "*agreement_id": "int() NOT NULL",
      "+property_id": "int()",
      "start_date": "date() NOT NULL",
      "end_date": "date()",
      "clauses": "text()",
      "security_deposit_refunded": "boolean() NOT NULL"
    },
    "MaintenanceRecord": {
      "*maintenance_id": "int() NOT NULL",
      "+property_id": "int()",
      "date": "date() NOT NULL",
      "description": "text() NOT NULL",
      "cost": "decimal(10,2) NOT NULL",
      "contractor": "varchar(100)"
    }
  },
  "relations": [
    "Property:agent_id *--1 Agent:agent_id",
    "Property:owner_id *--1 Owner:owner_id",
    "Property:renter_id ?--1 Renter:renter_id",
    "Payment:property_id *--1 Property:property_id",
    "RentalAgreement:property_id 1--1 Property:property_id",
    "MaintenanceRecord:property_id *--1 Property:property_id"
  ],
  "rankAdjustments": "",
  "label": "Real Estate Agency Management System"
}