{
  "tables": {
    "Agent": {
      "*agent_id": "int() NOT NULL",
      "first_name": "varchar(100)",
      "last_name": "varchar(100)",
      "email": "varchar(100)"
    },
    "Owner": {
      "*owner_id": "int() NOT NULL",
      "first_name": "varchar(100)",
      "last_name": "varchar(100)",
      "email": "varchar(100)"
    },
    "Renter": {
      "*renter_id": "int() NOT NULL",
      "first_name": "varchar(100)",
      "last_name": "varchar(100)",
      "email": "varchar(100)"
    },
    "Property": {
      "*property_id": "int() NOT NULL",
      "+agent_id": "int()",
      "+owner_id": "int()",
      "+renter_id": "int()",
      "address": "varchar(200)",
      "city": "varchar(100)",
      "state": "varchar(100)",
      "zip_code": "varchar(20)"
    },
    "Payment": {
      "*payment_id": "int() NOT NULL",
      "+property_id": "int()",
      "amount": "decimal(10,2) NOT NULL",
      "payment_date": "date()",
      "payment_month": "varchar(20)",
      "payer_account": "varchar(100)",
      "receiver_account": "varchar(100)"
    }
  },
  "relations": [
    "Property:agent_id *--1 Agent:agent_id",
    "Property:owner_id *--1 Owner:owner_id",
    "Property:renter_id *--1 Renter:renter_id",
    "Payment:property_id *--1 Property:property_id"
  ],
  "rankAdjustments": "",
  "label": "Real Estate Agency Management System"
}