{
  "tables": {
    "Agent": {
      "*agent_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "agreements_signed": "int()"
    },
    "Owner": {
      "*owner_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "contact_info": "varchar(200)"
    },
    "Renter": {
      "*renter_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "contact_info": "varchar(200)"
    },
    "Property": {
      "*property_id": "int() NOT NULL",
      "+owner_id": "int()",
      "+agent_id": "int()",
      "+renter_id": "int()",
      "address": "varchar(200) NOT NULL",
      "rent_price": "decimal(10,2) NOT NULL"
    },
    "Payment": {
      "*payment_id": "int() NOT NULL",
      "+property_id": "int()",
      "payment_date": "date() NOT NULL",
      "amount": "decimal(10,2) NOT NULL",
      "month_paid_for": "varchar(20) NOT NULL",
      "received_by_agency": "boolean() NOT NULL",
      "passed_to_owner": "boolean() NOT NULL"
    }
  },
  "relations": [
    "Property:owner_id 1--* Owner:owner_id",
    "Property:agent_id 1--* Agent:agent_id",
    "Property:renter_id ?--1 Renter:renter_id",
    "Payment:property_id 1--* Property:property_id"
  ],
  "rankAdjustments": "",
  "label": "Real Estate Agency Management System"
}