{
  "tables": {
    "Agent": {
      "*agent_id": "int() NOT NULL",
      "username": "varchar(100) NOT NULL",
      "password": "varchar(100) NOT NULL",
      "role": "varchar(50) NOT NULL"
    },
    "Owner": {
      "*owner_id": "int() NOT NULL",
      "name": "varchar(200) NOT NULL",
      "contact_info": "varchar(200)"
    },
    "Renter": {
      "*renter_id": "int() NOT NULL",
      "name": "varchar(200) NOT NULL",
      "contact_info": "varchar(200)"
    },
    "Property": {
      "*property_id": "int() NOT NULL",
      "+owner_id": "int()",
      "+agent_id": "int()",
      "address": "varchar(300) NOT NULL",
      "last_maintenance_date": "date()"
    },
    "RentalAgreement": {
      "*agreement_id": "int() NOT NULL",
      "+property_id": "int()",
      "+renter_id": "int()",
      "start_date": "date() NOT NULL",
      "end_date": "date()",
      "clauses": "text()",
      "security_deposit_refunded": "boolean()"
    },
    "Payment": {
      "*payment_id": "int() NOT NULL",
      "+agreement_id": "int()",
      "payment_date": "date() NOT NULL",
      "payment_method": "varchar(50) NOT NULL",
      "amount": "decimal(10,2) NOT NULL",
      "payment_account": "varchar(200)",
      "receiving_account": "varchar(200)",
      "payment_month": "varchar(20) NOT NULL"
    },
    "Rating": {
      "*rating_id": "int() NOT NULL",
      "+rated_by_id": "int()",
      "+rated_entity_id": "int()",
      "rating_value": "int() NOT NULL",
      "rated_entity_type": "varchar(50) NOT NULL"
    }
  },
  "relations": [
    "Property:owner_id 1--* Owner:owner_id",
    "Property:agent_id 1--* Agent:agent_id",
    "RentalAgreement:property_id 1--1 Property:property_id",
    "RentalAgreement:renter_id 1--* Renter:renter_id",
    "Payment:agreement_id 1--1 RentalAgreement:agreement_id",
    "Rating:rated_by_id *--1 Agent:agent_id",
    "Rating:rated_entity_id *--1 Owner:owner_id",
    "Rating:rated_entity_id *--1 Renter:renter_id",
    "Rating:rated_entity_id *--1 Agent:agent_id"
  ],
  "rankAdjustments": "",
  "label": "Real Estate Agency Management System"
}