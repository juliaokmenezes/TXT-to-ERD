{
  "tables": {
    "Agent": {
      "*agent_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)"
    },
    "Owner": {
      "*owner_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)"
    },
    "Property": {
      "*property_id": "int() NOT NULL",
      "+owner_id": "int()",
      "+agent_id": "int()",
      "address": "varchar(200) NOT NULL",
      "last_maintenance_date": "date() NOT NULL"
    },
    "Renter": {
      "*renter_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)",
      "salary": "decimal(10,2)"
    },
    "RentalAgreement": {
      "*agreement_id": "int() NOT NULL",
      "+property_id": "int()",
      "+renter_id": "int()",
      "start_date": "date() NOT NULL",
      "end_date": "date() NOT NULL",
      "clauses": "text()",
      "deposit_refunded": "boolean() NOT NULL",
      "guarantor_name": "varchar(100)"
    },
    "Payment": {
      "*payment_id": "int() NOT NULL",
      "+agreement_id": "int()",
      "payment_date": "date() NOT NULL",
      "amount": "decimal(10,2) NOT NULL",
      "payment_method": "varchar(50) NOT NULL",
      "payment_month": "varchar(20) NOT NULL",
      "sender_account": "varchar(50)",
      "receiver_account": "varchar(50)"
    },
    "Referral": {
      "*referral_id": "int() NOT NULL",
      "+referring_agent_id": "int()",
      "+referred_agent_id": "int()",
      "referral_date": "date() NOT NULL",
      "commission_amount": "decimal(10,2) NOT NULL"
    }
  },
  "relations": [
    "Property:owner_id 1--1 Owner:owner_id",
    "Property:agent_id 1--* Agent:agent_id",
    "RentalAgreement:property_id 1--1 Property:property_id",
    "RentalAgreement:renter_id 1--* Renter:renter_id",
    "Payment:agreement_id 1--* RentalAgreement:agreement_id",
    "Referral:referring_agent_id 1--* Agent:agent_id",
    "Referral:referred_agent_id 1--* Agent:agent_id"
  ],
  "rankAdjustments": "",
  "label": "Real Estate Agency Database"
}