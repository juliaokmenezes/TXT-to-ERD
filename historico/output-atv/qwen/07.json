{
  "tables": {
    "Agent": {
      "*agent_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)"
    },
    "Owner": {
      "*owner_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)"
    },
    "Renter": {
      "*renter_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)"
    },
    "Property": {
      "*property_id": "int() NOT NULL",
      "address": "varchar(200) NOT NULL",
      "price": "decimal(10,2) NOT NULL",
      "+owner_id": "int() NOT NULL",
      "+agent_id": "int() NOT NULL",
      "+current_renter_id": "int()"
    },
    "Payment": {
      "*payment_id": "int() NOT NULL",
      "amount": "decimal(10,2) NOT NULL",
      "payment_date": "date() NOT NULL",
      "month": "varchar(20) NOT NULL",
      "+property_id": "int() NOT NULL",
      "+renter_id": "int() NOT NULL",
      "received_by_owner": "boolean NOT NULL"
    }
  },
  "relations": [
    "Property:owner_id 1--* Owner:owner_id",
    "Property:agent_id 1--1 Agent:agent_id",
    "Property:current_renter_id ?--1 Renter:renter_id",
    "Payment:property_id 1--1 Property:property_id",
    "Payment:renter_id 1--1 Renter:renter_id"
  ],
  "rankAdjustments": "",
  "label": "Real Estate Agency Database"
}