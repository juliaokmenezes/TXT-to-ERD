{
  "tables": {
    "Agent": {
      "*agent_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)"
    },
    "Owner": {
      "*owner_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)"
    },
    "Renter": {
      "*renter_id": "int() NOT NULL",
      "name": "varchar(100) NOT NULL",
      "email": "varchar(100)",
      "phone": "varchar(20)"
    },
    "Property": {
      "*property_id": "int() NOT NULL",
      "+owner_id": "int()",
      "+agent_id": "int()",
      "address": "varchar(200) NOT NULL",
      "city": "varchar(100) NOT NULL",
      "state": "varchar(100) NOT NULL",
      "zip_code": "varchar(20) NOT NULL"
    },
    "RentalAgreement": {
      "*agreement_id": "int() NOT NULL",
      "+property_id": "int()",
      "+renter_id": "int()",
      "start_date": "date() NOT NULL",
      "end_date": "date() NOT NULL",
      "clauses": "text()",
      "deposit_refunded": "boolean()"
    },
    "Payment": {
      "*payment_id": "int() NOT NULL",
      "+agreement_id": "int()",
      "payment_date": "date() NOT NULL",
      "amount": "decimal(10,2) NOT NULL",
      "month": "varchar(20) NOT NULL",
      "payer_account": "varchar(100) NOT NULL",
      "receiver_account": "varchar(100) NOT NULL"
    },
    "Bill": {
      "*bill_id": "int() NOT NULL",
      "+property_id": "int()",
      "type": "varchar(50) NOT NULL",
      "payer": "varchar(50) NOT NULL",
      "amount": "decimal(10,2) NOT NULL",
      "due_date": "date() NOT NULL"
    }
  },
  "relations": [
    "Property:owner_id 1--* Owner:owner_id",
    "Property:agent_id 1--* Agent:agent_id",
    "RentalAgreement:property_id 1--* Property:property_id",
    "RentalAgreement:renter_id 1--* Renter:renter_id",
    "Payment:agreement_id 1--* RentalAgreement:agreement_id",
    "Bill:property_id 1--* Property:property_id"
  ],
  "rankAdjustments": "",
  "label": "Real Estate Management System"
}