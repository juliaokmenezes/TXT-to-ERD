{
    "tabelas": {
        "CorretorImoveis": {
            "*id_corretorImoveis": "int() NOT NULL",
            "nome": "varchar(100) NOT NULL",
            "numeroTelefone": "char(20) NOT NULL",
            "email": "varchar(50) NOT NULL"
        },
        "Proprietario": {
            "*id_proprietario": "int() NOT NULL",
            "nome": "varchar(100) NOT NULL",
            "numeroTelefone": "char(20) NOT NULL",
            "email": "varchar(50) NOT NULL"
        },
        "Inquilino": {
            "*id_inquilino": "int() NOT NULL",
            "nome": "varchar(100) NOT NULL",
            "numeroTelefone": "char(20) NOT NULL",
            "salario": "decimal(10,2) NOT NULL",
            "email": "varchar(50) NOT NULL"
        },
        "Imovel": {
            "*id_imovel": "int() NOT NULL",
            "endereco": "varchar(200) NOT NULL",
            "valorAluguel": "decimal(10,2) NOT NULL",
            "+id_proprietario": "int() NOT NULL",
            "+id_corretorImoveis": "int() NOT NULL",
            "disponivelParaAluguel": "boolean() NOT NULL"
        },
        "Locacao": {
            "*id_locacao": "int() NOT NULL",
            "+id_imovel": "int() NOT NULL",
            "+id_inquilino": "int() NOT NULL",
            "+id_inspecao": "int() NOT NULL",
            "dataInicioLocacao": "date() NOT NULL",
            "dataFimLocacao": "date()",
            "estadoInicialImovel": "text()",
            "valorDeposito": "decimal(10,2) NOT NULL"
        },
        "Pagamentos": {
            "*id_pagamento": "int() NOT NULL",
            "+id_locacao": "int() NOT NULL",
            "valorAluguel": "decimal(10,2) NOT NULL",
            "contaPagamento": "varchar(50) NOT NULL",
            "contaRecebimento": "varchar(50) NOT NULL",
            "dataPagamento": "date() NOT NULL"
        },
        "Fiador": {
            "*id_fiador": "int() NOT NULL",
            "+id_locacao": "int() NOT NULL"
        },
        "Inspecao": {
            "+id_inspecao": "int() NOT NULL",
            "+id_imovel": "int() NOT NULL",
            "danos": "text()",
            "reparosRealizados": "boolean() NOT NULL"
        }
    },

    "relacoes": [
        "Imovel:id_corretorImoveis 1--* CorretorImoveis:id_corretorImoveis",
        "Imovel:id_proprietario 1--* Proprietario:id_proprietario",
        "Imovel:id_imovel *--1 Locacao:id_imovel",
        "Inquilino:id_inquilino *--1 Locacao:id_inquilino",
        "Locacao:id_locacao *--1 Pagamentos:id_locacao",
        "Locacao:id_locacao 1--* Fiador:id_locacao",
        "Inspecao:id_imovel 1--* Imovel:id_imovel",
        "Locacao:id_inspecao 1--1 Inspecao:id_inspecao"
    ],
    "ajustesHierarquicos": "",
    "rotulo": "10"
}